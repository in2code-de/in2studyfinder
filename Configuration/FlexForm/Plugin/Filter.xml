<T3DataStructure>
    <sheets>
        <main>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:in2studyfinder/Resources/Private/Language/locallang_db.xlf:listView</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.flexform.studyCourseDetailPage>
                        <exclude>1</exclude>
                        <label>LLL:EXT:in2studyfinder/Resources/Private/Language/locallang_db.xlf:detailView</label>
                        <config>
                            <type>group</type>
                            <allowed>pages</allowed>
                            <size>1</size>
                            <maxitems>1</maxitems>
                            <minitems>1</minitems>
                        </config>
                    </settings.flexform.studyCourseDetailPage>
                    <settings.flexform.select.department>
                        <label>LLL:EXT:in2studyfinder/Resources/Private/Language/locallang_db.xlf:department</label>
                        <config>
                            <type>select</type>
                            <renderType>selectCheckBox</renderType>
                            <maxitems>9999</maxitems>
                            <size>5</size>
                            <minitems>0</minitems>
                            <foreign_table>tx_in2studyfinder_domain_model_department</foreign_table>
                            <foreign_table_where>and tx_in2studyfinder_domain_model_department.sys_language_uid in
                                (-1,0) order by tx_in2studyfinder_domain_model_department.title
                            </foreign_table_where>
                        </config>
                    </settings.flexform.select.department>
                    <settings.flexform.select.faculty>
                        <label>LLL:EXT:in2studyfinder/Resources/Private/Language/locallang_db.xlf:faculty</label>
                        <config>
                            <type>select</type>
                            <renderType>selectCheckBox</renderType>
                            <maxitems>9999</maxitems>
                            <size>5</size>
                            <minitems>0</minitems>
                            <foreign_table>tx_in2studyfinder_domain_model_faculty</foreign_table>
                            <foreign_table_where>and tx_in2studyfinder_domain_model_faculty.sys_language_uid in
                                (-1,0) order by tx_in2studyfinder_domain_model_faculty.title
                            </foreign_table_where>
                        </config>
                    </settings.flexform.select.faculty>
                    <settings.flexform.select.graduation>
                        <label>LLL:EXT:in2studyfinder/Resources/Private/Language/locallang_db.xlf:academicDegree
                        </label>
                        <config>
                            <type>select</type>
                            <renderType>selectCheckBox</renderType>
                            <maxitems>9999</maxitems>
                            <size>5</size>
                            <minitems>0</minitems>
                            <foreign_table>tx_in2studyfinder_domain_model_graduation</foreign_table>
                            <foreign_table_where>and tx_in2studyfinder_domain_model_graduation.sys_language_uid in
                                (-1,0) order by tx_in2studyfinder_domain_model_graduation.title
                            </foreign_table_where>
                        </config>
                    </settings.flexform.select.graduation>
                </el>
            </ROOT>
        </main>
    </sheets>
</T3DataStructure>
